{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Liquibase": {
    "Run": true,
    "ExecutablePath": "C:\\Program Files\\liquibase\\liquibase.bat",
    "WorkingDirectory": "D:\\SRC\\FinstarDatabase\\Database\\Migrations",
    "ContextFilter": "any",
    "LabelFilter": "c-any or c-hbl"
  },
  "SqlServer": {
    "HostName": "localhost",
    "InstanceName": "SQLDEVELOPER",
    "DatabaseName": "FsFinDev",
    "Encrypt": true,
    "TrustedServerCertificate": true,
    "IntegratedSecurity": true
  },
  "Misc": {
    "DoNotCreateTriggersForTables": "AsText,AsUnconfirmed,AsHistory,AsNavigationIndex,AsNavigationIndexSub",
    "DoNotCreateGetDetailAndGetListForStoredProcedures": "AsText,AsUnconfirmed,AsHistory,AsNavigationIndex,AsNavigationIndexSub"
  }
}
